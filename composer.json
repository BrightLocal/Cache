    "apcu",
    "file",
    "memcached",
    "memcache",
    "mysql",
    "mongo",
    "redis"
  ],
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/desarrolla2/Cache/",
  "authors": [
    {
      "name": "Daniel GonzÃ¡lez",
      "homepage": "http://desarrolla2.com/"
    }
  ],
  "require": {
    "php": ">=5.6.0",
    "psr/simple-cache": "^1.0"
  },
  "require-dev": {
    "php": ">=7.1.0",
    "ext-apcu": "*",
    "ext-mysqli": "*",
    "ext-memcached": "*",
    "phpunit/phpunit": "^7.0",
    "predis/predis": "~1.0.0",
    "nesbot/carbon": "^1.29",
    "mongodb/mongodb": "^1.3"
  },
  "autoload": {
    "psr-4": {
